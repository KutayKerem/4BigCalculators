<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FenerbahceAccounts">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBaseline_toBaselineOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/fb4" />

    <Button
        android:id="@+id/bol"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="23dp"
        android:onClick="divide"
        android:text="/"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/carp"
        app:layout_constraintTop_toBottomOf="@+id/çıkar" />

    <Button
        android:id="@+id/çıkar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="7dp"
        android:onClick="extraction"
        android:text="-"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:iconTint="#F4F4F4"
        app:layout_constraintBottom_toTopOf="@+id/bol"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/topla2"
        app:layout_constraintTop_toBottomOf="@+id/sayi2" />

    <Button
        android:id="@+id/carp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="135dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="22dp"
        android:onClick="bump"
        android:text="*"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/temizle2"
        app:layout_constraintEnd_toStartOf="@+id/bol"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topla2" />

    <Button
        android:id="@+id/topla2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="135dp"
        android:layout_marginTop="601dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="9dp"
        android:onClick="tocollect"
        android:text="+"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/carp"
        app:layout_constraintEnd_toStartOf="@+id/çıkar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="57dp"
        android:onClick="back"
        android:text="Geri"
        android:textColor="#C6C4C4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/temizle2"
        app:layout_constraintTop_toBottomOf="@+id/bol" />

    <Button
        android:id="@+id/temizle2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="101dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="57dp"
        android:onClick="clear"
        android:text="Sil"
        android:textColor="#C6C4C4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carp" />

    <EditText
        android:id="@+id/sayi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="329dp"
        android:layout_marginBottom="26dp"
        android:ems="10"
        android:hint="1.Sayıyı giriniz.."
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/sayi2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/sayi2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="154dp"
        android:ems="10"
        android:hint="2.Sayıyı giriniz.."
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/çıkar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sayi" />

    <TextView
        android:id="@+id/sonuc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="84dp"
        android:textAlignment="center"
        android:textColor="#C6C4C4"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/sayi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>