<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TrabzonsporAccounts">

    <Button
        android:id="@+id/bol"
        android:layout_width="50dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="144dp"
        android:onClick="divide"
        android:text="/"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/çıkar" />

    <Button
        android:id="@+id/çıkar"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:layout_marginTop="599dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="95dp"
        android:onClick="extraction"
        android:text="-"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:iconTint="#F4F4F4"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/carp"
        android:layout_width="50dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:onClick="bump"
        android:text="*"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/bol"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topla2" />

    <Button
        android:id="@+id/topla2"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:layout_marginBottom="95dp"
        android:onClick="tocollect"
        android:text="+"
        android:textColor="#C6C4C4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/temizle2"
        app:layout_constraintEnd_toStartOf="@+id/çıkar"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sayi2" />

    <Button
        android:id="@+id/back"
        android:layout_width="84dp"
        android:layout_height="0dp"
        android:layout_marginEnd="112dp"
        android:layout_marginBottom="22dp"
        android:onClick="back"
        android:text="Geri"
        android:textColor="#C6C4C4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/çıkar" />

    <Button
        android:id="@+id/temizle2"
        android:layout_width="84dp"
        android:layout_height="0dp"
        android:layout_marginBottom="22dp"
        android:onClick="clear"
        android:text="Sil"
        android:textColor="#C6C4C4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/back"
        app:layout_constraintHorizontal_bias="0.77"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topla2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ts" />

    <EditText
        android:id="@+id/sayi2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="69dp"
        android:ems="10"
        android:hint="2.Sayıyı giriniz.."
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/topla2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sayi"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/sayi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="425dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:hint="1.Sayıyı giriniz.."
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/sayi2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/sonuc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="84dp"
        android:textAlignment="center"
        android:textColor="#C6C4C4"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/sayi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>